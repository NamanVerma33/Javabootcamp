import java.util.Scanner;
public class vote
{
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int age;
		System.out.println("Enter Your Age");
		age=sc.nextInt();
		if(age>18)
		{
		    System.out.println("You can Vote");
		}
		else
		{
		    System.out.println("You cannot vote");
		}
		
	}
}
